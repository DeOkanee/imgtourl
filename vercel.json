{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "25mb" }
    }
  ],
  "routes": [
    {
      "src": "/hasil",
      "dest": "app.py"
    },
    {
      "src": "/result",
      "dest": "app.py"
    },
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production",
    "FLASK_APP": "app"
  }
}
